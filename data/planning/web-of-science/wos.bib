@article{ WOS:000481880700020,
Author = {Shams, Hamed and Sadeghfam, Amin and Rostami, Naghi and Tohidi, Sajjad},
Title = {Exact approach for charging of PEVs with V2G capability to improve
   micro-grid reliability},
Journal = {IET GENERATION TRANSMISSION \& DISTRIBUTION},
Year = {2019},
Volume = {13},
Number = {16},
Pages = {3690-3695},
Month = {AUG 20},
Abstract = {Nowadays, the major proportion of fossil fuels is consumed in the
   transportation systems. Plug-in electric vehicles (PEVs) can play a
   significant role in reducing the petrol consumption. A large amount of
   additional electrical power demand needed for the charging of PEVs may
   adversely affect the electric grid reliability. Using the charging
   station in the micro-grids (MGs) to minimise the total system operation
   cost in the presence of PEVs with vehicle to grid (V2G) capability is
   becoming vitally important. In this work, a practical model for charging
   strategy of PEVs based on typical charge and discharge profiles given by
   the battery producers is considered. The problem is constrained by the
   loss of load probability index of reliability. The proposed strategy
   makes it possible to handle the generation shortage due to the outage of
   conventional units and intermittency of wind turbine units. Mixed
   Integer Non-linear programming is used for the optimal planning of MG in
   the presence of PEVs. Results show that an appropriate charge and
   discharge strategy may lead to a significant improvement in the
   reliability of the considered MG as well as a reduction in the
   operational cost of the total system.},
Type = {Article},
Affiliation = {Rostami, N (Corresponding Author), Univ Tabriz, Fac Elect \& Comp Engn, Tabriz, Iran.
   Shams, Hamed; Sadeghfam, Amin; Rostami, Naghi; Tohidi, Sajjad, Univ Tabriz, Fac Elect \& Comp Engn, Tabriz, Iran.},
DOI = {10.1049/iet-gtd.2018.6752},
Author-Email = {n-rostami@tabrizu.ac.ir},
