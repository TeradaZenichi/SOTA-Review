}

@article{ WOS:000540752500016,
Author = {Rahmani, Mojtaba and Hosseinian, Seyed Hossein and Abedi, Mehrdad},
Title = {Stochastic two-stage reliability-based Security Constrained Unit
   Commitment in smart grid environment},
Journal = {SUSTAINABLE ENERGY GRIDS \& NETWORKS},
Year = {2020},
Volume = {22},
Month = {JUN},
Abstract = {In the realm of the smart grid, there are new issues that are needed to
   be addressed. However, these new concerns may have an adverse effect on
   the complexity of the operation of the network, they present great
   opportunities to the network operators for a better service, in terms of
   lower costs and enhanced security. In this paper, the integration of the
   coordinated aggregated Plug-in Electric Vehicle (PEV) fleets and Demand
   Response Programs (DRPs) into power systems is studied using the
   stochastic reliability-based Security Constrained Unit Commitment
   (SCUC). The proposed SCUC program minimizes network operation costs
   while determining the best strategy for deployment of PEVs and DRPs.
   Moreover, the proposed approach analyze the effect of these resources on
   the adequacy of the power system by considering reliability metrics
   Being a mobile power storing device enables the PEVs to provide their
   energy to the network when called upon. Moreover, DRPs can mitigate
   power consumption during the peak load or in case of a contingency.
   Optimal integration of these resources could reduce the network
   operating costs and improve security. A two-stage stochastic
   mixed-integer programming model is employed for power system modeling in
   a smart grid environment. In the proposed model, PEV fleets are modeled
   as virtual power plants that can provide electric power while DRPs are
   assumed to only affect the power consumption of the customers. The
   proposed paradigm is applied to the IEEE 6-bus and IEEE Reliability Test
   System (RTS) to illustrate its effectiveness. Numerical analyses confirm
   the effectiveness of the proposed strategy in lessening the cost of
   network operations, better performance in case of contingencies and a
   higher level of reliability. (c) 2020 Elsevier Ltd. All rights reserved.},
Type = {Article},
Affiliation = {Hosseinian, SH (Corresponding Author), Amirkabir Univ Technol, Dept Elect Engn, Tehran, Iran.
   Rahmani, Mojtaba; Hosseinian, Seyed Hossein; Abedi, Mehrdad, Islamic Azad Univ, Qazvin Branch, Fac Elect Biomed \& Mechatron Engn, Qazvin, Iran.
   Hosseinian, Seyed Hossein; Abedi, Mehrdad, Amirkabir Univ Technol, Dept Elect Engn, Tehran, Iran.},
