@article{ WOS:001522934400005,
Author = {Pandit, Deepak and Pandit, Dilip and Nguyen, Nga},
Title = {Reliability Assessment of Power Systems in the Presence of Large-Scale
   Electric Vehicle Integration},
Journal = {IEEE TRANSACTIONS ON INDUSTRY APPLICATIONS},
Year = {2025},
Volume = {61},
Number = {4},
Pages = {6707-6720},
Month = {JUL-AUG},
Abstract = {The rising environmental concerns and depletion of fossil fuels have
   promoted the integration of electric vehicles (EVs) into modern power
   grids. EVs can serve either as a load or storage, presenting a
   dual-faceted challenge to power system planning and operation. As a
   result, there is a growing need for comprehensive reliability assessment
   in the presence of EVs. This paper proposes improved reliability
   assessment frameworks for systems with large-scale electric vehicles
   (EVs) operating in battery exchange (BE) and plug-in modes. Initially, a
   reliability model for BE electric vehicles (BEVs) is formulated using a
   time-series aggregated EV user load profile, BE station operation
   strategy, and off-peak battery exchange mode of BEVs. Additionally, a
   random-sampling-based operational algorithm is developed for plug-in EVs
   (PEVs), considering their charging, discharging, and grid support modes.
   A composite reliability assessment framework is then developed,
   incorporating an optimization model that accounts for the aggregated
   operations of BEVs and PEVs. The effect of EVs on the system reliability
   is assessed using the mixed-timing sequential Monte Carlo simulation on
   the IEEE RTS-79 and the RTS-GMLC systems. Simulation results show that
   up to 18\% improvement in reliability indices is achieved with a 15\%
   penetration of BEVs operating in off-peak mode. Also, the same
   penetration of PEVs with grid-support modes yields up to 14\%
   improvement in system reliability. Finally, the impacts of BEV and PEV
   integration on the loading and reliability of transmission lines and
   power transformers are analyzed.},
Type = {Article},
Affiliation = {Nguyen, N (Corresponding Author), Univ Wyoming, Dept Elect Engn \& Comp Sci, Laramie, WY 82071 USA.
   Pandit, Deepak, PRE Inc, Power Delivery Grp, Denver, CO 80122 USA.
   Pandit, Dilip; Nguyen, Nga, Univ Wyoming, Dept Elect Engn \& Comp Sci, Laramie, WY 82071 USA.},
