@article{ WOS:001197539100001,
Author = {Zhou, Jinghua and Weng, Zhipeng and Li, Jin and Song, Xiaotong},
Title = {Reliability evaluation, planning, and economic analysis of microgrid
   with access to renewable energy and electric vehicles},
Journal = {ELECTRIC POWER SYSTEMS RESEARCH},
Year = {2024},
Volume = {230},
Month = {MAY},
Abstract = {The integration of renewable energy (RE) and electric vehicles (EVs)
   into microgrids enhances energy sustainability, but their variability
   complicates capacity planning. Therefore, a capacity planning method is
   proposed from the perspective of reliability evaluation and economic
   analysis. Firstly, an EVs model with load characteristics is developed.
   Then, the reliability evaluation process and economic model are
   presented. Finally, the capacity planning schemes are compared and
   selected in the modified RBTS BUS6 F4 system. The results show that the
   probability indices will be optimized by increasing RE capacity with a
   certain energy storage system (ESS) configuration, but the system
   average interruption frequency index (SAIFI) shows non-monotonic change.
   Taking Microgrid A as an example, the peak of SAIFI can be reduced by
   63.17 \% when ESS configuration change from 1.0 MW \& sdot;h/0.1 MW to
   3.0 MW \& sdot;h/1.5 MW, which indicates that reasonable ESS
   configuration can effectively reduce the impact of SAIFI on microgrid.
   Comparing the reliability and economy of Microgrid B and Microgrid C,
   when the microgrid is connected at a lower load level, Microgrid C can
   reduce the total cost by 1,764,999 CNY while maintaining the same
   reliability as Microgrid B. The research results can provide reference
   for microgrid planning. (c) 2017 Elsevier Inc. All rights reserved.},
Type = {Article},
Affiliation = {Weng, ZP (Corresponding Author), North China Univ Technol, Inverter Technol Engn Res Ctr Beijing, Beijing 100144, Peoples R China.
