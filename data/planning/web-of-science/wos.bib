@article{ WOS:000599940100001,
Author = {Tadayon-Roody, Pooya and Ramezani, Maryam and Falaghi, Hamid},
Title = {Multi-objective locating of electric vehicle charging stations
   considering travel comfort in urban transportation system},
Journal = {IET GENERATION TRANSMISSION \& DISTRIBUTION},
Year = {2021},
Volume = {15},
Number = {5},
Pages = {960-971},
Month = {MAR},
Abstract = {Environmental concerns of using fossil fuel vehicles and recent
   developments in electric vehicle (EV) technology have attracted the
   attention of the international community to use EVs. A design strategy
   is proposed by simultaneously considering a distribution network and an
   urban transportation network, that maximises drivers' travel comfort in
   urban trips and reduces costs of both charging station (CS) installation
   and losses. The proposed strategy meets various constraints such as,
   properly locating CSs in the city, considering traffic volume, taking
   the shortest route, reducing charging waiting time, limits of bus
   voltages, power balance, the permissible loading of lines. The travel
   comfort index corresponds to a situation in which the driver does not
   experience a depleted battery and successfully finish the trip. Since
   the movement of vehicles over the course of a day does not follow any
   particular pattern, unscented transformation is used to investigate the
   uncertainty in different probabilistic parameters of EVs. Moreover, by
   clustering the locations of EVs, the optimal locations of EV CSs are
   determined over the course of the day (in several time steps) using a
   multi-objective function based on a GA. The simulation results of the
   sample urban transportation and distribution networks confirm the
   efficacy of the proposed method.},
Type = {Article},
Affiliation = {Ramezani, M (Corresponding Author), Univ Birjand, Fac Elect \& Comp Engn, POB 97175-376, Birjand, Iran.
   Tadayon-Roody, Pooya; Ramezani, Maryam; Falaghi, Hamid, Univ Birjand, Fac Elect \& Comp Engn, POB 97175-376, Birjand, Iran.},
DOI = {10.1049/gtd2.12072},
EarlyAccessDate = {DEC 2020},
Author-Email = {mramezani@birjand.ac.ir},
Times-Cited = {19},
Unique-ID = {WOS:000599940100001},
