@article{ WOS:001534556900039,
Author = {Zeng, Rong and Liu, Jiayan and Li, Yong and Lee, Kwang Y. and Hu, Sijia
   and Cao, Yijia},
Title = {Joint Optimal Charging Scheduling for Multiple EV Battery Swapping
   Stations Involving the Finite-State Modeling of Charging Bay},
Journal = {IEEE TRANSACTIONS ON TRANSPORTATION ELECTRIFICATION},
Year = {2025},
Volume = {11},
Number = {4},
Pages = {8784-8796},
Month = {AUG},
Abstract = {Battery swapping stations (BSSs) present a promising electric vehicle
   (EV) power supplement solution. However, the joint modeling and optimal
   scheduling for multiple BSSs is complex and rarely researched,
   especially when the idle state of charging bays (CBs) is non-negligible.
   In this article, for the distribution network (DN) with multiple BSSs, a
   joint optimal charging scheduling strategy considering the idle state of
   CB is developed to ensure the system's safe operation and BSSs' economic
   charging. In particular, a three-layered coupled model for multiple BSSs
   is constructed by modeling on the CB, BSS, and DN layers, which is used
   to visualize each BSS's state quantity changes and their charging
   behaviors' impacts on DN. To further improve the accuracy of this model,
   the CB's idle state is given special consideration by developing a
   three-stage modeling method, which can detailedly describe the finite
   state transfer of CBs. In addition, to deal with the joint scheduling of
   multiple BSSs (JS-MBSS) problem upon the constructed model, a bi-level
   solving algorithm is proposed to convert the complex JS-MBSS into a
   solvable mixed integer quadratic constraint program (MIQCP). Finally,
   the proposed strategy and two others are verified via the modified IEEE
   33-bus system. The results suggest that by adopting the proposed
   strategy, the total charging cost and the cumulative voltage fluctuation
   are reduced by 39.42\% and 30.84\%, respectively, which verifies the
   proposal's positive benefits in stabilizing grid operation and enhancing
   the charging economy.},
