}

@article{ WOS:000723373100009,
Author = {Abdalrahman, Ahmed and Zhuang, Weihua},
Title = {QoS-Aware Capacity Planning of Networked PEV Charging Infrastructure},
Journal = {IEEE OPEN JOURNAL OF VEHICULAR TECHNOLOGY},
Year = {2020},
Volume = {1},
Pages = {116-129},
Abstract = {Plug-in electric vehicle (PEV) charging infrastructure is necessary to
   accommodate the rapid increase in PEV penetration rate. Capacity
   planning of PEV charging infrastructure (EVCI) must ensure not only a
   satisfactory charging service for PEV users but also a reliable
   operation of the power grid. In this paper, we propose a
   quality-of-service (QoS) aware capacity planning of EVCI. In particular,
   the proposed framework accounts for the link between the charging QoS
   and the power distribution network (PDN) capability. Towards this end,
   we firstly optimize charging facility sizes to achieve a targeted QoS
   level. Then, we minimize the integration cost for the PDN by attaining
   the most cost-effective allocation of the energy storage systems (ESSs)
   and/or upgrading the PDN substation and feeders. Additionally, we
   capture the correlation between the occupation levels of neighboring
   charging facilities and the blocked PEV user behaviors. We model the
   EVCI as a queuing network with finite capacity, and utilize the
   non-stationary queuing models to study the temporal variability of the
   PEV charging demand. A network of charging facilities is used to
   demonstrate the effectiveness of the proposed framework.},
Type = {Article},
Affiliation = {Abdalrahman, A (Corresponding Author), Univ Waterloo, Dept Elect \& Comp Engn, Waterloo, ON N2L 3G1, Canada.
   Abdalrahman, Ahmed; Zhuang, Weihua, Univ Waterloo, Dept Elect \& Comp Engn, Waterloo, ON N2L 3G1, Canada.},
DOI = {10.1109/OJVT.2020.2979820},
Author-Email = {aabdalra@uwaterloo.ca},
Times-Cited = {10},
Unique-ID = {WOS:000723373100009},
DA = {2025-12-09},
}

@article{ WOS:000927894800001,
Author = {Haji-Aghajani, Ehsan and Hasanzadeh, Saeed and Heydarian-Forushani,
   Ehsan},
Title = {A novel framework for planning of EV parking lots in distribution
   networks with high PV penetration},
Journal = {ELECTRIC POWER SYSTEMS RESEARCH},
Year = {2023},
Volume = {217},
Month = {APR},
Abstract = {In this paper, a method based on calculating the probability of electric
   vehicles (EVs) entering each parking lot is proposed for the long-term
   planning of EV parking lots. Also, a method for optimal scheduling of
   charging and discharging EVs to maximize profit has been proposed. EV
   growth percentage during the planning horizon has been modeled using the
   point estimation method. Also, in order to solve the problem, a
   non-dominated sorting genetic algorithm has been used. The proposed
   method has been implemented on a distribution network of 69 buses. The
   results show the effectiveness of the proposed method in maximizing the
   profit from the construction of parking lots and saving energy
   consumption to reach the parking lot. In addition, the location,
   capacity, and optimal time of building new parking lots or increasing
   the capacity of existing parking lots have been optimally determined.},
Type = {Article},
Affiliation = {Hasanzadeh, S (Corresponding Author), Qom Univ Technol, Dept Elect \& Comp Engn, Qom, Iran.
