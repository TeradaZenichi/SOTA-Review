DOI = {10.1016/j.segan.2023.101198},
EarlyAccessDate = {FEB 2024},
Article-Number = {101198},
Author-Email = {mostafa.barani@ntnu.no},
Times-Cited = {8},
Unique-ID = {WOS:001167876200001},
DA = {2025-12-09},
}

@article{ WOS:001424657900001,
Author = {Chen, Zhongbo and Liang, Liang and Lu, Chao},
Title = {Optimized Coordination of Electric Vehicles, Distributed Compensation
   Devices, and Distributed Generation for Risk Mitigation in Radial
   Distribution Networks},
Journal = {IET RENEWABLE POWER GENERATION},
Year = {2025},
Volume = {19},
Number = {1},
Month = {JAN},
Abstract = {The charging demand of electric vehicles (EVs), intelligent and flexible
   compensation units, and renewable energy generation are key components
   of the smart grid framework. This paper aims to evaluate the benefits of
   coordinating distributed compensation units while mitigating the risks
   associated with the uncertainties arising from the aggregation of
   distributed generation and EV charging loads within the smart grid.
   Linear models for electric spring (ES) were developed to facilitate
   their integration into collaborative operational strategies.
   Subsequently, probabilistic load flow (PLF) analysis was combined with
   branch power flow (BPF) equations to assess voltage fluctuations and the
   stochastic distribution of energy demand. The proposed methodology aims
   to optimize coordinated operations for risk mitigation. Finally, case
   studies were carried out to verify the effectiveness of the proposed
   optimization model and provide further insights into its practical
   applications.},
Type = {Article},
Affiliation = {Liang, L (Corresponding Author), Harbin Inst Technol Shenzhen, Coll Artificial Intelligence, Shenzhen, Peoples R China.
   Chen, Zhongbo; Liang, Liang, Harbin Inst Technol Shenzhen, Coll Artificial Intelligence, Shenzhen, Peoples R China.
