}

@article{ WOS:000899058000001,
Author = {Kurukuru, Varaha Satya Bharath and Khan, Mohammed Ali and Singh, Rupam},
Title = {Electric Vehicle Charging/Discharging Models for Estimation of Load
   Profile in Grid Environments},
Journal = {ELECTRIC POWER COMPONENTS AND SYSTEMS},
Year = {2023},
Volume = {51},
Number = {3},
Pages = {279-295},
Month = {FEB 7},
Abstract = {The increasing electric vehicles (EVs) and their integration with the
   electrical networks to achieve the charging demands has highly affected
   the operation of the power system. These effects are due to the
   increased and variable amount of electricity consumption caused by the
   irregular charging patterns of EVs. As the EV penetration level
   increases with such irregular charging patterns, the overall load
   profile in a network will increase resulting in load peaks. To overcome
   this, a well-planned charging-discharging algorithm is needed for the EV
   users and the power suppliers. In light of the above issues and
   requirements, this paper proposes a Markov Chain Monte Carlo (MCMC)
   simulation-based EV charging (EVC) behavior models and
   charging-discharging power prediction models. The proposed methods
   analyze the traveling behavior of EVs, and their charging patterns to
   make EV a flexible load. This helps in understanding the variable
   electricity consumption and mitigating the peak loads and load
   variations. Further, to assess the operation of the proposed methods,
   the charging-discharging power prediction is carried out for a large EV
   fleet based on the historical data. The power predictions are carried
   out for 100 days and the convergence of the proposed algorithm is
   targeted to be kept below 0.5\% to achieve optimal power prediction. The
   results showed the credibility of the proposed approach in identifying
   the daily average charging-discharging load curves.},
Type = {Article},
Affiliation = {Khan, MA (Corresponding Author), Brno Univ Technol, Fac Elect Engn \& Commun, Dept Elect Power Engn, Brno 110025, Czech Republic.
   Kurukuru, Varaha Satya Bharath, Silicon Austria Labs GmbH, Power Elect Res Div, Villach, Austria.
   Khan, Mohammed Ali, Brno Univ Technol, Fac Elect Engn \& Commun, Dept Elect Power Engn, Brno, Czech Republic.
   Singh, Rupam, Alpen Adria Univ Klagenfurt, Inst Intelligent Syst Technol, Klagenfurt, Austria.
   Khan, Mohammed Ali, Brno Univ Technol, Fac Elect Engn \& Commun, Dept Elect Power Engn, Brno 110025, Czech Republic.},
DOI = {10.1080/15325008.2022.2146811},
EarlyAccessDate = {DEC 2022},
Author-Email = {khan@vut.cz},
Times-Cited = {11},
Unique-ID = {WOS:000899058000001},
DA = {2025-12-09},
}

@article{ WOS:001523487600002,
Author = {Gupta, Pranda Prasanta and Kalkhambkar, Vaiju and Kumari, Nikita and
   Awati, Jayashree Samai and Sharma, Himanshu},
Title = {Optimal Scheduling of EV Charging Station With Renewable Generation
   Considering Line Outages and Network Losses},
Journal = {IEEE TRANSACTIONS ON INDUSTRY APPLICATIONS},
Year = {2025},
Volume = {61},
Number = {4},
Pages = {5626-5636},
Month = {JUL-AUG},
Abstract = {The uncertain renewable energy sources (RESs) and grid integration of EV
   charging stations create several operational challenges in the power
   system. This research provides a comprehensive mathematical optimization
   model for EV charging scheduling that integrates stochastic
   network-constrained unit commitment (NCUC) with N-1 criteria and network
   losses. A generalized generation distribution factor (GGDF) for the
   pre-and post-contingency selection technique is applied based on the DC
   power flow model to address the uncertain parameters. A line outage
   distribution factors (LODF) method is proposed for the post-contingency
   constraints to reduce the computational burden due to many scenarios and
   N-k line outages. In addition, the bender decomposition approach is
   employed to simplify the problem complexity. Hence, this work evaluates
   the impact of transportation networks for EV charging stations and N-k
   outages in a proposed model for economic and technical benefits. The
   validity of the NCUC model is substantiated through verification using
   IEEE 24-bus systems. The economic benefits shared by EV charging
   stations and RES with post-contingency LODF were reduced by about 5.91\%
   in scheduling costs as compared to the conventional preventive
   GGDF-based approach.},
Type = {Article},
Affiliation = {Gupta, PP (Corresponding Author), GLA Univ, Dept Elect Engn, Mathura 281406, Uttar Pradesh, India.
   Gupta, Pranda Prasanta, GLA Univ, Dept Elect Engn, Mathura 281406, Uttar Pradesh, India.
   Kalkhambkar, Vaiju, Rajarambapu Inst Technol, Dept Elect Engn, Sangli 415409, Maharashtra, India.
   Kumari, Nikita; Awati, Jayashree Samai, Rajarambapu Inst Technol, Dept Elect \& Telecommun, Sangli 415409, Maharashtra, India.
