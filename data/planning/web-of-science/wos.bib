@article{ WOS:000634772600001,
Author = {Zhang, Jingqi and Wang, Shu and Zhang, Cuo and Luo, Fengji and Dong,
   Zhao Yang and Li, Yingliang},
Title = {Planning of electric vehicle charging stations and distribution system
   with highly renewable penetrations},
Journal = {IET ELECTRICAL SYSTEMS IN TRANSPORTATION},
Year = {2021},
Volume = {11},
Number = {3},
Pages = {256-268},
Month = {SEP},
Abstract = {With the increasing prevalence of electric vehicles (EVs), the EV
   charging station (EVCS) and power distribution have become a coupled
   physical system. A multi-objective planning model is developed herein
   for the sizing and siting of EVCSs and the expansion of a power
   distribution network with high wind power penetration. The objectives of
   the planning model are to minimise the total cost of investment and
   energy losses of the distribution system while maximising the total
   captured traffic flow. The uncertainties associated with wind power
   sources are considered. Additionally, the uncertainties in EV daily
   charging loads are also important concerns in the optimisation of the
   planning model. To model the EV load uncertainties, a recent scenario
   generation (SG) method is adopted. Further, a multi-objective
   optimisation tool, Multi-Objective Natural Aggregation Algorithm
   (MONAA), is introduced to obtain the final solutions of the planning
   model. The simulations based on coupled 54-node distribution network and
   25-node traffic network systems are conducted to verify the efficiency
   of the proposed model and the effectiveness of SG-based MONAA.},
Type = {Article},
Affiliation = {Zhang, JQ (Corresponding Author), UNSW, Sch Elect Engn \& Telecommun, Sydney, NSW 2052, Australia.
   Zhang, Jingqi; Zhang, Cuo; Dong, Zhao Yang, Univ New South Wales, Sch Elect Engn \& Telecommun, Sydney, NSW, Australia.
   Luo, Fengji, Univ Sydney, Sch Civil Engn, Sydney, NSW, Australia.
   Wang, Shu, Changan Univ, Sch Automobile, Xian, Peoples R China.
   Li, Yingliang, Xian Shiyou Univ, Sch Elect Engn, Xian, Peoples R China.},
DOI = {10.1049/els2.12022},
EarlyAccessDate = {MAR 2021},
Author-Email = {jingqi.zhang1@student.unsw.edu.au},
Times-Cited = {15},
