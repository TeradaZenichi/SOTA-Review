}

@article{ WOS:001416192500043,
Author = {Nogueira, Rennan de Melo and Manassero Junior, Giovanni and Tiferes,
   Rodrigo Rozenblit},
Title = {A Methodology to Support the Planning of Energy Distribution
   Reinforcements Based on a SpaceTime Analysis of Electric Vehicle
   Charging},
Journal = {IEEE TRANSACTIONS ON TRANSPORTATION ELECTRIFICATION},
Year = {2025},
Volume = {11},
Number = {1},
Pages = {163-175},
Month = {FEB},
Abstract = {The electric vehicle (EV) market is growing, enhancing city environments
   with reduced noise and improved sustainability. However, this growth
   poses challenges for power distribution networks, initially designed for
   stationary loads. In this context, this article introduces an innovative
   method for subsidizing reinforcement planning in power distribution
   networks by analyzing EV charging over time and space. This novel
   methodology is exemplified in a Sao Paulo City central region case,
   utilizing an open-source power distribution simulator. Diverse market
   penetration scenarios, recharge demands, and patterns (residential,
   commercial, and power station recharges) are considered. The outcomes of
   this approach offer insights into future energy distribution network
   diagnoses, facilitating optimal reinforcement planning and grid
   modernization. It mitigates over-estimations and potential tariff
   increases affecting all consumers, not just EV owners, while proactively
   addressing potential future challenges.},
