@article{ WOS:001453080500014,
Author = {Saber, Ahmed Yousuf and Kabir, Kazi Meharajul and Khandelwal, Tanuj},
Title = {Optimum Resource Sizing and Operation in Virtual Power Plant Framework:
   VPP optimization and operation},
Journal = {IEEE ELECTRIFICATION MAGAZINE},
Year = {2025},
Volume = {13},
Number = {1},
Pages = {66-74},
Month = {MAR},
Abstract = {The optimum size and operation of virtual power plants (VPPs) are
   essential for sustainable green power systems in modern society.
   Renewable energy (mainly wind and solar) is intermittent and
   non-dispatchable. The aggregator of available energy storage systems and
   controllable loads, including heating, cooling, EV charging/discharging,
   etc., can mitigate those issues in the VPP framework and maintain system
   balance. The VVP platform's wind speed, solar radiation, and load demand
   are highly dynamic. Without intelligent coordination, it is impossible
   to determine the optimal size of the resources with minimum operation
   cost. An overestimate of resources suffers from unnecessary excess
   capital cost, which might not be feasible. On the other hand, an
   underestimate of resources builds an unreliable VPP. It is a
   multi-objective complex problem considering the minimization of capital
   and operation costs as objectives, subject to many constraints,
   including net zero emission. Moreover, an intelligent energy management
   system (EMS) is deployed considering present and near future dynamic
   forecasted demand profile, state of charge (SoC) of storage, and power
   availability from renewables for reliability, and maintains net zero
   energy and emission. All the dispatch and transaction communications in
   VPP are committed to using blockchain. This paper uses an intelligent
   multi-objective strategy to design and optimize a generalized VPP model.
   Historical wind speed, solar radiation, and load profiles over a long
   period are used as inputs for the model. An intelligent energy
   management system for the dispatch of resources is carried out. Results
   will show that the proposed method can build an effective VPP
   considering costs, emissions, efficiency, and reliability.},
Type = {Article},
Affiliation = {Saber, AY (Corresponding Author), Elect Transient \& Anal Program, Irvine, CA 92618 USA.
   Saber, Ahmed Yousuf; Kabir, Kazi Meharajul; Khandelwal, Tanuj, Elect Transient \& Anal Program, Irvine, CA 92618 USA.},
