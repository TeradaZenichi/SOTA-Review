@article{ WOS:000428478300023,
Author = {El-naggar, Mohammed Fathy and Elgammal, Adel Abdelaziz Abdelghany},
Title = {Multi-Objective Optimal Predictive Energy Management Control of
   Grid-Connected Residential Wind-PV-FC-Battery Powered Charging Station
   for Plug-in Electric Vehicle},
Journal = {JOURNAL OF ELECTRICAL ENGINEERING \& TECHNOLOGY},
Year = {2018},
Volume = {13},
Number = {2},
Pages = {742-751},
Month = {MAR},
Abstract = {Electric vehicles (EV) are emerging as the future transportation vehicle
   reflecting their potential safe environmental advantages. Vehicle to
   Grid (V2G) system describes the hybrid system in which the EV can
