@article{ WOS:000483813000003,
Author = {Battapothula, Gurappa and Yammani, Chandrasekhar and Maheswarapu, Sydulu},
Title = {Multi-objective optimal planning of FCSs and DGs in distribution system
   with future EV load enhancement},
Journal = {IET ELECTRICAL SYSTEMS IN TRANSPORTATION},
Year = {2019},
Volume = {9},
Number = {3},
Pages = {128-139},
Month = {SEP},
Abstract = {Current trends suggest that electrical vehicle (EV) is a promising
   technology for road transportation. There is a substantial increase in
   the number of EVs due to improved energy efficiency and reduction in
   environmental impact as compared with internal combustion engine
   vehicles. The improper planning of fast charging stations (FCSs) and
   distributed generations (DGs) hurts the distribution system. So the
   distribution system operator has a significant challenge to identify the
   optimal location and sizing of FCSs in the distribution power network.
   This study presents optimal planning of FCSs and DGs with the account of
   the present and future increase in EV population. A multi-objective
   optimisation problem is formulated for optimal planning of FCSs and DGs
   with the objective of minimising the voltage deviation, distribution
   network power loss, DGs cost and the energy consumption of EV users.
   This problem is solved for different levels of increase in EV population
   for different cases. A novel hybrid shuffled frog leap-teaching and
   learning based optimisation algorithm is proposed and implemented to
   solve the considered multi-objective problem. The performance of the
   proposed algorithm is compared with prior-art algorithms in the
   literature.},
Type = {Article},
Affiliation = {Battapothula, G (Corresponding Author), Natl Inst Technol Warangal, Elect Engn Dept, Warangal, Andhra Pradesh, India.
   Battapothula, Gurappa; Yammani, Chandrasekhar; Maheswarapu, Sydulu, Natl Inst Technol Warangal, Elect Engn Dept, Warangal, Andhra Pradesh, India.},
DOI = {10.1049/iet-est.2018.5066},
Author-Email = {gurubattapothula@student.nitw.ac.in},
