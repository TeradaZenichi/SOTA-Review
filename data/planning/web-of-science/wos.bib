@article{ WOS:001568805500001,
Author = {Yang, Aixin and Ren, Fei and Zhang, Guiqing and Li, Chengdong},
Title = {Two-stage collaborative optimization strategy for electric vehicle
   charging scheduling based on the optimal scale of photovoltaic system},
Journal = {COMPUTERS \& ELECTRICAL ENGINEERING},
Year = {2025},
Volume = {128},
Number = {A},
Month = {DEC},
Abstract = {Electric vehicles (EVs) are entering thousands of households, as they
   contribute to sustainable energy development and effectively alleviate
   environmental degradation. However, the unregulated charging behavior of
   EVs has a significant impact on the power quality of the distribution
   network, accompanied by integration with renewable energy sources, which
   puts double pressure on the grid. To tackle these problems, this paper
   proposes a two-stage collaborative optimization strategy for EV charging
   scheduling with photovoltaic (PV) system. In the upper-stage
   optimization, a Variational Bayesian Gaussian mixture model (VB-GMM) is
   presented to learn the real EV charging event distribution, and the
   generated charging events are used to achieve the optimization of the
   on-site PV system scale. In the lower-stage optimization, using the
   optimal PV system scale obtained from the upper-stage optimization to
   calibrate the output power, a collaborative optimization of EV charging
   scheduling and PV system using the Lagrange multiplier method is
   proposed. The effectiveness of the proposed strategy is analyzed and
   validated using real-world, full-day charging data collected from
   charging facilities at the parking lots of Caltech campus. Experimental
   results show that the optimized charging scheduling strategy can reduce
   the charging cost, alleviate the peak-to-valley difference of the grid,
   and promote coordination and complementary with renewable energy
   sources.},
Type = {Article},
Affiliation = {Zhang, GQ (Corresponding Author), Shandong Jianzhu Univ, Jinan 250101, Peoples R China.
   Yang, Aixin; Zhang, Guiqing; Li, Chengdong, Shandong Jianzhu Univ, Sch Informat \& Elect Engn, Shandong Key Lab Intelligent Bldg Technol, Jinan 250101, Peoples R China.
