@article{ WOS:000615031100001,
Author = {Younesi, Abdollah and Shayeghi, Hossein and Siano, Pierluigi and Safari,
   Amin and Alhelou, Hassan Haes},
Title = {Enhancing the Resilience of Operational Microgrids Through a Two-Stage
   Scheduling Strategy Considering the Impact of Uncertainties},
Journal = {IEEE ACCESS},
Year = {2021},
Volume = {9},
Pages = {18454-18464},
Abstract = {This article deals with the stochastic scheduling of a microgrid (MG) to
   balance the economical and resilience metrics. In the proposed model,
   the MG resilience indices are integrated into the economic criteria to
   ensure the resilience of MG operation alongside the main MG actors'
   profit/loss. The MG fragility index, recovery efficiency index, MG
   voltage index, and lost load index are considered in the proposed
   planning model. Further, to make the results more realistic, the
   uncertainties associated with energy price and wind production,
   alongside with planning of energy storage systems and electric vehicles
   parking lots are considered. To achieve a better solution for the
   security-constraint operation of MG, AC network equations are included
   in the system modeling. Finally, a large-scale MG based on the IEEE-33
   bus testbed is utilized to evaluate the effectiveness of the proposed
   stochastic scheduling program.},
Type = {Article},
Affiliation = {Shayeghi, H (Corresponding Author), Univ Mohaghegh Ardabili, Energy Management Res Ctr, Ardebil 5619911367, Iran.
