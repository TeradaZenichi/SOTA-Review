@article{ WOS:000694010000002,
Author = {Yang, Yang and Wang, Shu},
Title = {Resilient residential energy management with vehicle-to-home and
   photovoltaic uncertainty},
Journal = {INTERNATIONAL JOURNAL OF ELECTRICAL POWER \& ENERGY SYSTEMS},
Year = {2021},
Volume = {132},
Month = {NOV},
Abstract = {Resilient operation of residential buildings is an emerging topic. This
   paper proposes a resilient home energy management strategy to enable
   residential houses to implement self-power supply during a planned grid
   outage period. The proposed strategy utilizes the energy backup
   capability of plug-in hybrid electric vehicles (PHEV) to accommodate
   residential solar photovoltaic (PV) source and power the house in the
   presence of grid outage event. The proposed method also harnesses the
   load shifting flexibility of the smart home through a user disutility
   model. A stochastic programming approach based on Wasserstein distance
   metric and K-medoids based is applied to handle the residential
   photovoltaic solar power uncertainty. Simulation results show the
   proposed method can effectively enhance the residential resilience.},
Type = {Article},
Affiliation = {Wang, S (Corresponding Author), Changan Univ, Sch Automobile, Xian 710064, Peoples R China.
