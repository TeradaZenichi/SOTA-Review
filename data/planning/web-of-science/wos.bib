@article{ WOS:001579059500003,
Author = {Yang, Yuyan and Xu, Xiao and Luo, Yichen and Liu, Youbo and Xiang, Yue
   and Hu, Weihao and Liu, Junyong},
Title = {Enhancing Resilience of Urban Distribution Networks: Integrating
   Agent-Based V2G Response Model for Extreme Event Restoration},
Journal = {IEEE TRANSACTIONS ON TRANSPORTATION ELECTRIFICATION},
Year = {2025},
Volume = {11},
Number = {5},
Pages = {12342-12356},
Month = {OCT},
Abstract = {Amid power outages during extreme events, the urban distribution network
   (DN) endeavors to minimize power loss and facilitate restoration by
   leveraging emerging technologies, particularly with the proliferation of
   electric vehicles (EVs). This work prioritizes bolstering DN resilience
   through network reconfiguration (NR), emergency repair crew (ERC)
   dispatch, and vehicle-to-grid (V2G) response, providing topology, repair
   schedule, and power support. To measure the power support effect of EVs,
   an agent-based model (ABM) estimates EV aggregation for V2G response,
   considering key factors such as EV number, type, self-interested
   decision-making, behaviors, and traffic conditions. By integrating these
   parameters at macro and micro levels, agents interact with one another,
   including EVs, charging stations (CSs), CS aggregators, DN operators,
   and personalized trip advisors (PTAs). PTA navigates EVs to locate the
   nearest CS. Considering the interdependence of the three restoration
   methods, coordinated optimization is designed as a mixed integer linear
   programming (MILP) with the IEEE 33 and PG\&E 69 bus systems. The case
   study shows that the collaboration of these methods enhances load
   restoration efficiency and reduces operation costs. Sensitivity analysis
   on the number and type proportion of EVs and the number of ERCs offers
   guidance for DN operators in recovery planning.},
Type = {Article},
Affiliation = {Xu, X (Corresponding Author), Sichuan Univ, Coll Elect Engn, Chengdu, Peoples R China.
   Yang, Yuyan; Xu, Xiao; Luo, Yichen; Liu, Youbo; Xiang, Yue; Liu, Junyong, Sichuan Univ, Coll Elect Engn, Chengdu, Peoples R China.
