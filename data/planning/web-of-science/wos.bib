@article{ WOS:000948958100001,
Author = {Alizadeh, Mohammad and Jafari-Nokandi, Meysam},
Title = {A bi-level resilience-oriented islanding framework for an active
   distribution network incorporating electric vehicles parking lots},
Journal = {ELECTRIC POWER SYSTEMS RESEARCH},
Year = {2023},
Volume = {218},
Month = {MAY},
Abstract = {Due to the increase in natural disasters, islanding and reconfiguring
   plans are used to enhance the resiliency of distribution networks (DN).
   Due to the increase in Plug-in Electric Vehicle (PEV) penetration, the
   power system's resilience can be increased by scheduling PEV charging
   and discharging in electrified Parking Lots (PLs); So this paper
   presents a bi-level optimization model that considers the electrical and
   transportation networks simultaneously. The Upper-Level (UL) problem
   specifies the optimal DN islanding plan to maximize the amount of load
   restored utilizing the optimal charge/discharge management of PEVs in
   PLs. The closest energized PL to the deenergized destination PLs and the
   number of PEVs in energized PLs are specified in the Lower-Level (LL)
   problem. The proposed bi-level framework is converted into a
   single-level model utilizing the Karush-KuhnTucker optimality
   constraints due to the linearity of the LL problem. The efficiency of
   the proposed framework is verified using GAMS software applying the
   IEEE-33 bus distribution system, and the 25-node traffic network.
   Analysis of the simulation results shows that the optimal islanding
   decision considering the PEV drivers' willingness to stop in energized
   PLs and optimal charge and discharge management of these PEVs enhances
   the resilience of the DN.},
Type = {Article},
Affiliation = {Alizadeh, M (Corresponding Author), Imam Khomeini Naval Univ, Fac Elect \& Comp Engn, Noshahr, Iran.
   Alizadeh, Mohammad, Imam Khomeini Naval Univ, Fac Elect \& Comp Engn, Noshahr, Iran.
