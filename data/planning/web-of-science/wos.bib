@article{ WOS:001553590900002,
Author = {Gao, Xiaowen and Song, Dongming and Jiang, Leilei and Zhou, Kun},
Title = {Grid-aware charging infrastructure deployment: A hybrid monte Carlo and
   improved hummingbird algorithm for cost-efficiency and load balancing},
Journal = {ELECTRIC POWER SYSTEMS RESEARCH},
Year = {2025},
Volume = {249},
Month = {DEC},
Abstract = {The large-scale grid connection of electric vehicles is increasingly
   putting pressure on the distribution network, and the problem of load
   imbalance poses serious challenges to the stability and operational
   efficiency of the power grid. The current research mainly focuses on
   single facility planning or simple optimization models, which are
   difficult to solve the complex interaction problems between
   spatiotemporal charging dynamics, user service demands, and grid
   constraints. To this end, this study proposes an innovative framework
   for collaborative planning of charging and swapping facilities. Firstly,
   Monte Carlo simulation of spatiotemporal load distribution is used to
   coordinate the allocation of charging and swapping facility resources,
   breaking through the limitations of traditional isolated planning.
   Secondly, a multi-objective optimization model is constructed, using a
   weighted sum strategy to coordinate construction costs, user waiting
   time, and grid stability indicators, achieving dynamic balance of
   conflicting objectives. Furthermore, the Improved Hummingbird Algorithm
   (IAHA) is developed, which integrates chaotic mapping to enhance
   population diversity, and combines non-uniform mutation to achieve
   adaptive global local search, effectively solving high-dimensional
   complex programming problems. A case study in a certain region of
   northern China shows that compared to particle swarm optimization (PSO)
   algorithm, this framework can reduce annual infrastructure costs by 62.3
   million yuan, improve convergence speed by 40\%, and significantly
   improve the load balancing characteristics of the power grid. This study
   provides a solid theoretical foundation and practical solutions for
   optimizing charging infrastructure, which has important guiding
   significance for the flexible operation of smart grids and the
   improvement of user service quality.},
Type = {Article},
Affiliation = {Song, DM (Corresponding Author), Linyi Univ, Sch Automat \& Elect Engn, Linyi, Shandong, Peoples R China.
   Gao, Xiaowen; Song, Dongming; Zhou, Kun, Linyi Univ, Sch Automat \& Elect Engn, Linyi, Shandong, Peoples R China.
   Jiang, Leilei, State Grid Shandong EV Serv Co Ltd, Jinan 250000, Shandong, Peoples R China.},
