   Awad, Ahmed S. A., Ain Shams Univ, Elect Power \& Machines Dept, Fac Engn, Cairo 11566, Egypt.},
DOI = {10.1109/JSYST.2020.3012939},
Author-Email = {amer111@uwindsor.ca
   mazzouz@uwindsor.ca
   azab@uwindsor.ca
   a.samir@eng.asu.edu.eg},
Times-Cited = {29},
