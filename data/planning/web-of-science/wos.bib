@article{ WOS:001534932100001,
Author = {Boudellal, Amay and Ziras, Charalampos and Larsen, Emil Mahler and
   Marinelli, Mattia},
Title = {Techno-economic comparison of grid reinforcement and battery-buffered
   electric vehicle fast charging stations},
Journal = {SUSTAINABLE ENERGY GRIDS \& NETWORKS},
Year = {2025},
Volume = {43},
Month = {SEP},
Abstract = {Connecting fast charging stations to the electric grid usually requires
   an upgrade with new transformer stations and cables to accommodate the
   higher loading. Battery-based solutions can be operated following
   different control objectives and support the grid to reduce the
   investment need in conventional assets. A grid planning algorithm is
   developed to perform this comparison. The impact of the rollout of
   distributed energy resources on a Danish urban grid is assessed, based
   on a forecasted scenario and the geographical distribution of new loads
   and generators until 2045. Fast charging stations are added to this
   rollout, and the resulting grid issues are mitigated using a
   conventional grid reinforcement method. In a second iteration, batteries
   are installed in combination with fast charging stations to support
   them. The remaining capacity is used to solve the previously mentioned
   grid issues, using controllers that achieve good performance. The
   technical and economic impact of the conventional grid reinforcement and
   the battery-based one are compared in a cost-benefit analysis. Depending
   on cost sensitivities, the battery-based alternative can range from
   being 40 \% cheaper to over 1400 \% more expensive than a conventional
   grid extension-despite potential investment deferrals, which are often
   outweighed by higher capital costs and energy losses. Across all battery
   connection topologies and price sensitivities, the conventional grid
   upgrade solution remains the most cost-efficient one in 89 \% of all
   investigated cases, with the battery-buffered one being only competitive
   in edge cases.},
Type = {Article},
Affiliation = {Ziras, C (Corresponding Author), Tech Univ Denmark, Wind \& Energy Syst Dept, Bldg 325, DK-2800 Lyngby, Denmark.
   Boudellal, Amay; Larsen, Emil Mahler, Utiligize ApS, Sankt Gertruds Str 5,1, DK-1129 Copenhagen K, Denmark.
   Ziras, Charalampos; Marinelli, Mattia, Tech Univ Denmark, Wind \& Energy Syst Dept, Bldg 325, DK-2800 Lyngby, Denmark.},
