}

@article{ WOS:001076692100001,
Author = {Alghamdi, Abdullah Ali M. and Jayaweera, Dilan},
Title = {Resilience of modern power distribution networks with active
   coordination of EVs and smart restoration},
Journal = {IET GENERATION TRANSMISSION \& DISTRIBUTION},
Year = {2023},
Volume = {17},
Number = {21},
Pages = {4836-4857},
Month = {NOV},
Abstract = {In this modern era of cyber-physical-social systems, there is a need of
   dynamic coordination strategies for electric vehicles (EVs) to enhance
   the resilience of modern power distribution networks (MPDNs). This paper
   proposes a two-stage EV coordination framework for MPDN smart
   restoration. The first stage is to introduce a novel proactive EV
   prepositioning model to optimize planning prior to a rare event, and
   thereby enhance the MPDN survivability in its immediate aftermath. The
   second stage involves creating an advanced spatial-temporal EV dispatch
   model to maximize the number of available EVs for discharging, thereby
   improving the MPDN recovery after a rare event. The proposed framework
   also includes an information system to further enhance MPDN resilience
   by effectively organizing data exchange among intelligent transportation
   system and smart charging system, and EV users. In addition, a novel
   bidirectional geographic graph is proposed to optimize travel plans,
   covering a large penetration of EVs and considering variations in
   traffic conditions. The effectiveness is assessed on a modified IEEE
   123-node test feeder with real-world transportation and charging
   infrastructure. The results demonstrate a significant improvement in
   MPDN resilience with smart restoration strategies. The validation and
   sensitivity analyses evidence a significant superiority of the proposed
   framework.
   In the realm of cyber-physical-social systems, existing research lacks a
   comprehensive framework to optimize coordination strategies for electric
   vehicles (EVs) and enhance the resilience of modern power distribution
   networks (MPDNs). To address this gap, a two-stage EV coordination
   framework is proposed: The first stage focuses on proactive EV
   prepositioning to improve the MPDN survivability immediately after a
   rare event, while the second stage involves advanced spatial-temporal EV
   dispatching to maximize the EV availability for discharge and enhance
   the MPDN recovery. The framework incorporates an information system for
   dynamic data exchange, utilizes a novel geographic graph for optimizing
   EV routes, and demonstrates significant improvements in MPDN resilience
   through real-world evaluations on a modified IEEE 123 node test
   feeder.image},
